    "octobercms",
    "theme",
    "laravel",
    "bootstrap"
  ],
  "author": "Prismify",
  "license": "MIT",
  "homepage": "https://github.com/prismify/oc-bootstrap-theme",
  "devDependencies": {
    "laravel-mix": "^3.0.0"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.6"
